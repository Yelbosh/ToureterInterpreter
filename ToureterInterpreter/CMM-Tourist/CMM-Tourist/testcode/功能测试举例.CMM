	//插入排序，降序输出
    real a[6];
    a[0] = 9.1;
    a[1] = 6.1;
    a[2] = 5.2;
    a[3] = 4.3;
    a[4] = 2.5454;
    a[5] = 7.222;

    int i = 0;
    int j = 0;
    real temp = 0;

    while(i<6){
	j = i;
	temp = a[i];
	    
        while(0<j){
        	if(temp<a[j-1]){
        		a[j] = a[j-1];
        	    a[j-1] = temp;
        	}
        	j = j-1;
        }

	i = i+1;
    }

write(a[0]);write(a[1]);write(a[2]);write(a[3]);write(a[4]);write(a[5]);
