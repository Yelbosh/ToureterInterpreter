例二：//冒泡排序，降序输出
    int a[6];
    a[0] = 9;
    a[1] = 5;
    a[2] = 6;
    a[3] = 4;
    a[4] = 2;
    a[5] = 7;

	    int j = 5;
	    int i = 0;
	    int temp = 0;
	while(0<j){
	    i = 0;
	    while(i<5){
	        if  (a[i]<a[i+1])
{
	        temp = a[i];
            a[i] = a[i+1];
            a[i+1] = temp;
            }
	        i = i+1;
	    }
	      j = j-1;
	}

write(a[0]);write(a[1]);write(a[2]);write(a[3]);write(a[4]);write(a[5]);
